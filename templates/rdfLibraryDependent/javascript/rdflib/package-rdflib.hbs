{
  "name": "{{npmModuleScope}}{{artifactPrefix}}{{artifactName}}{{artifactSuffix}}",
  "version": "{{artifactVersion}}",
  "description": "{{description}}",
  "main": "index.js",
{{#if supportBundling}}
  "scripts": {
    "dev": "webpack --config ./config/webpack.dev.config.js",
    "prod": "webpack --config ./config/webpack.prod.config.js"
  },
{{/if}}
  "author": {
    "name": "{{generatorName}}",
    "url": "https://github.com/inrupt/lit-artifact-generator-js"
  },
{{#if contributors}}
  "contributors": [
    {{#each contributors}}
    {"name": "{{this}}"}{{#unless @last}},{{/unless}}
    {{/each}}
  ],
{{/if}}
  "license": "MIT",
{{#if versioning}}
  "repository": {
    "type": "{{versioning.type}}",
    "url": "{{versioning.url}}"
  },
{{/if}}
  "dependencies": {
    "@inrupt/lit-vocab-term-rdflib": "{{litVocabTermVersion}}"
  }{{#if supportBundling}},
  "devDependencies": {
    "@babel/core": "{{babelCoreVersion}}",
    "babel-loader": "{{babelLoaderVersion}}",
    "webpack": "{{webpackVersion}}",
    "webpack-cli": "{{webpackCliVersion}}"
  }{{/if}}
}
