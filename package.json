{
  "name": "@lit/artifact-generator",
  "version": "1.0.0",
  "description": "A generator for building ontology artifacts",
  "main": "index.js",
  "bin": {
    "lit-artifact-generator": "./index.js"
  },
  "preferGlobal": true,
  "scripts": {
    "test": "npm run format && npm run lint && mkdir -p test/resources/output && nyc mocha --require mock-local-storage \"./test/**/*.test.js\"",
    "start": "node index.js",
    "format": "prettier --single-quote --trailing-comma es5 --write \"index.ts\" \"{src,test}/**/*.js\" \"!test/expected/**\"",
    "lint": "eslint src -c .eslintrc.json --ext js,jsx"
  },
  "author": "inrupt, Inc.",
  "contributors": [
    "Jarlath Holleran (https://github.com/jholleran)",
    "Pat McBennett (https://github.com/pmcb55)"
  ],
  "license": "MIT",
  "dependencies": {
    "@lit/vocab-term": "^1.0.11",
    "@lit/generated-vocab-common": "^1.2.9",
    "handlebars": "^4.1.2",
    "inquirer": "^6.4.1",
    "mock-local-storage": "^1.1.8",
    "rdf-ext": "^1.2.2",
    "rdf-fetch-lite": "^1.1.2",
    "rdf-parser-jsonld": "^1.1.0",
    "rdf-parser-n3": "^1.1.1",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.5.0",
    "del": "^4.1.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "sinon": "^7.3.2"
  },
  "directories": {
    "test": "test"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 98,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [],
    "reporter": [
      "lcov",
      "text"
    ],
    "require": [],
    "extension": [],
    "cache": true,
    "all": true,
    "report-dir": "./.nyc_output/alternative"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git@github.com:inrupt/lit-artifact-generator-js.git"
  }
}
