prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
prefix owl: <http://www.w3.org/2002/07/owl#>
prefix vann: <http://purl.org/vocab/vann/>
prefix dcterms: <http://purl.org/dc/terms/>
prefix lit_core: <https://w3id.org/lit/vocab/core#>

prefix pet_rock: <http://pat.com/vocab/my_app#>


pet_rock:
    a owl:Ontology, lit_core:Ontology ;
    owl:versionInfo "1.0.0" ;
    dcterms:creator "Pat McBennett" ;
    dcterms:date "2019/07/28" ;
    dcterms:title "Pet Rock vocabulary" ;
    dcterms:description "Vocabulary for Pet Rock collectors, including terms of their characteristics..." ;
    vann:preferredNamespacePrefix "pats_app" ;
    vann:preferredNamespaceUri "http://pat.com/vocab/my_app#" ;
    dcterms:license "http://www.opendatacommons.org/licenses/pddl/" .


pet_rock:PetRock a rdfs:Class ;
  rdfs:label "Pet Rock"@en ;
  rdfs:label "Pierre de compagnie"@fr ;
  rdfs:label "Pet Rock"@es ;
  rdfs:comment "The class of all rocks that this app considers excellent Pet's."@en ;
  rdfs:comment "La clase de todas las rocas que esta aplicación considera excelentes mascotas."@es .

pet_rock:shinyness a rdf:Property ;
  rdfs:label "Shinyness"@en ;
  rdfs:label "Brillo"@es ;
  rdfs:comment "How wonderfully shiny a rock is."@en ;
  rdfs:comment "Qué maravillosamente brillante es una roca."@es .

pet_rock:rarity a rdf:Property ;
  rdfs:label "Rarity"@en ;
  rdfs:label "Rareté"@fr ;
  rdfs:label "Rareza"@es ;
  rdfs:comment "The class of all rocks that this app considers excellent Pet's."@en ;
  rdfs:comment "The class of all rocks that this app considers excellent Pet's."@es .
